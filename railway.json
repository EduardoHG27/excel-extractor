{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pip install --upgrade pip && pip install -r requirements.txt"
  },
  "deploy": {
    "startCommand": "python manage.py migrate && gunicorn excel_extractor.wsgi --bind 0.0.0.0:$PORT --workers 2 --threads 2 --timeout 120"
  }
}