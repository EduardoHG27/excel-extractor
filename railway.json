{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pip install --upgrade pip && pip install -r requirements.txt"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "python manage.py migrate && gunicorn excel_extractor.wsgi",
    "healthcheckPath": "/",
    "healthcheckTimeout": 60
  }
}
